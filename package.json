{
  "name": "swagger_blocks_fastjson_api",
  "private": true,
  "scripts": {
    "api:gen": "npm run swagger:gen && npm run ts:gen",
    "swagger:gen": "bundle exec rails swagger:gen",
    "ts:gen": "openapi-generator generate -i ./tmp/swagger.json -g typescript-axios -o ./app/javascript/gen/ --additional-properties=\"modelPropertyNaming=snake_case\""
  },
  "dependencies": {
    "@rails/actioncable": "^6.0.0-alpha",
    "@rails/activestorage": "^6.0.0-alpha",
    "@rails/ujs": "^6.0.0-alpha",
    "@rails/webpacker": "^4.0.2",
    "axios": "^0.19.0",
    "turbolinks": "^5.2.0",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-router": "^3.0.6",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.1",
    "vuex-typescript-fsa": "^0.4.0"
  },
  "version": "0.1.0",
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^0.0.14-4.0.2",
    "@types/webpack-env": "^1.13.9",
    "prettier": "^1.18.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.2",
    "webpack-dev-server": "^3.3.1"
  }
}
